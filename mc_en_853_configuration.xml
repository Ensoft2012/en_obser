<?xml version = "1.0" encoding = "utf-8"?>

<configurations>
	<applications>
		<application>
			<name>MC_TOUCH_CONTROL</name>
			<log_list>
				<log>
					<name>MAIN</name>
					<enable>Y</enable>
					<path>C:\\ensoft\\logs</path>
					<log_name>TOUCH</log_name>
					<file_name>touch.log</file_name>
				</log>
			</log_list>
			<communication>
				<tcp_list>
					<tcp>
						<name>TCP_RECEIVE</name>
						<enable>Y</enable>
						<ipaddress>127.0.0.1</ipaddress>
						<port>10001</port>
					</tcp>
				</tcp_list>
				<udp_list>
					<udp>
						<name>UDP_RECEIVE</name>
						<enable>Y</enable>
						<ipaddress>127.0.0.1</ipaddress>
						<port>6000</port>
					</udp>
					<udp>
						<name>TO_TOUCH</name>
						<enable>Y</enable>
						<ipaddress>127.0.0.1</ipaddress>
						<port>6000</port>
					</udp>
					<udp>
						<name>TO_FDC</name>
						<enable>Y</enable>
						<ipaddress>127.0.0.1</ipaddress>
						<port>6001</port>
					</udp>
				</udp_list>
				<rs232_list>
					<rs232>
						<name>KEYBOARD</name>
						<enable>Y</enable>
						<port>COM1</port>
					</rs232>
				</rs232_list>
			</communication>
			<memory>
				<sharedmemory_list>
					<sharedmemory>
						<name>MEMORY_IMAGE</name>
						<enable>Y</enable>
						<connection_name>myImageSharedMemory</connection_name>
						<open_type>open_only</open_type>
						<access_mode>read_only</access_mode>
					</sharedmemory>
				</sharedmemory_list>
			</memory>
			<database>
				<whitedb_list>
					<whitedb>
						<name>WHITEDB</name>
						<enable>Y</enable>
						<connection_name></connection_name>
						<connection_string></connection_string>
					</whitedb>
				</whitedb_list>
			</database>
			<resource>
				<classification_list>
					<classification>
						<name>KE_CLASSIFICATION</name>
						<enable>Y</enable>
						<path>C:\\ensoft\\config\\ke_classifications_640.xml</path>
					</classification>					
				</classification_list>
				<image_list>
					<image>
						<name>KE_IMAGE_640</name>
						<enable>Y</enable>
						<path>C:\\ensoft\\config\\ke_images_640.xml</path>
					</image>					
				</image_list>
			</resource>			
		</application>
		<application>
			<name>MC_OPSERVER</name>
			<execute_list>
				<execute>
					<name>MC_853_CAPTURE</name>
					<enable>Y</enable>
					<path>C:\ensoft\bin\mc-capture.exe</path>
					<arguments></arguments>
					<console_show></console_show>
				</execute>
				<execute>
					<name>MC_853_TOUCH_CONTROL</name>
					<enable>Y</enable>
					<path>C:\ensoft\bin\mc-touch-control.exe</path>
					<arguments>COM1</arguments>
					<console_show></console_show>
				</execute>
				<execute>
					<name>MC_853_OCR</name>
					<enable>Y</enable>
					<path>C:\ensoft\bin\mc-ocr.exe</path>
					<arguments></arguments>
					<console_show></console_show>
				</execute>
				<execute>
					<name>MC_853_FDC</name>
					<enable>Y</enable>
					<path>C:\ensoft\bin\mc-ke-fdc.exe</path>
					<arguments>-c H01</arguments>
					<console_show></console_show>
				</execute>
				<execute>
					<name>MC_853_VIEWER</name>
					<enable>Y</enable>
					<path>C:\ensoft\bin\mc-viewer.exe</path>
					<arguments></arguments>
					<console_show></console_show>
				</execute>
			</execute_list>			
			<monitor_list>
				<monitor>
					<name>MC_CAPTURE</name>
					<enable>Y</enable>
					<process_name>mc-capture</process_name>
				</monitor>
				<monitor>
					<name>MC_KE_FDC</name>
					<enable>Y</enable>
					<process_name>mc-ke-fdc</process_name>
				</monitor>
			</monitor_list>
		</application>
	</applications>
</configurations>